<h2>Movies of Same Director</h2>
<table class="table table-striped col-md-12" id="movies">
  <thead>
    <tr>
      <th>Movie Title</th>
    </tr>
  </thead>
  <tbody>
    <% @movies.each do |movie| %>
      <tr>
        <td>
          <%= movie %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>